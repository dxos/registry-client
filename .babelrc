{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "10.10",
        },
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "import-graphql"
  ]
}
